<h2>Show participant</h2>

<ul>

  <li>
    <strong>Device:</strong>
    <%= @participant.device_id %>
  </li>

  <li>
    <strong>Nickname:</strong>
    <%= @participant.nickname %>
  </li>

</ul>

<span><%= link "Edit", to: participant_path(@conn, :edit, @participant) %></span>
<span><%= link "Back", to: participant_path(@conn, :index) %></span>
